{
  "name": "council-frontend",
  "version": "1.0.0",
  "scripts": {
    "start": "vite --host",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 80 --host",
    "typecheck": "vue-tsc --noEmit",
    "setenv": "node setenv.js",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path ../.gitignore",
    "lintnofix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path ../.gitignore --max-warnings 0"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.0-alpha.40",
    "@vuelidate/validators": "^2.0.0-alpha.28",
    "@vueuse/core": "^8.3.0",
    "axios": "^0.26.1",
    "pinia": "^2.0.13",
    "primeflex": "^3.1.3",
    "primeicons": "^5.0.0",
    "primevue": "^3.15.0",
    "toastr": "^2.1.4",
    "vue": "^3.2.33",
    "vue-axios": "^3.4.1",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.14",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "prettier": "^2.5.1",
    "sass": "^1.50.1",
    "typescript": "~4.6.3",
    "vite": "^2.9.15",
    "vue-tsc": "^0.34.17"
  }
}
